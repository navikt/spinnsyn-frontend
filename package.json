{
    "name": "spinnsyn-frontend",
    "version": "0.1.1",
    "private": true,
    "dependencies": {
        "@navikt/ds-css": "^1.3.17 ",
        "@navikt/ds-icons": "^1.3.17 ",
        "@navikt/ds-react": "^1.3.17 ",
        "@navikt/ds-tokens": "^1.3.17 ",
        "@navikt/nav-dekoratoren-moduler": "^1.7.0",
        "@navikt/next-api-proxy": "^1.3.0",
        "@navikt/next-logger": "^1.11.0",
        "@sentry/nextjs": "^7.15.0",
        "amplitude-js": "^8.21.1",
        "axe-core": "^4.4.3",
        "cookie": "^0.5.0",
        "cypress-axe": "^1.0.0",
        "dayjs": "^1.11.5",
        "html-react-parser": "^3.0.4",
        "jose": "^4.9.3",
        "next-logger": "^3.0.1",
        "openid-client": "^5.1.10",
        "pino-pretty": "^9.1.1",
        "prom-client": "^14.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.39.2",
        "typescript": "^4.8.4",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^5.14.0",
        "@navikt/eslint-config-teamsykmelding": "^1.0.0",
        "@types/amplitude-js": "^8.16.2",
        "@types/classnames": "^2.3.0",
        "@types/cookie": "^0.5.1",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.8.4",
        "@types/react": "^18.0.21",
        "@types/uuid": "^8.3.4",
        "cypress": "^10.10.0",
        "eslint": "^8.27.0",
        "eslint-config-next": "^12.3.1",
        "eslint-plugin-cypress": "^2.12.1",
        "jest": "^29.1.2",
        "jest-environment-jsdom": "^29.2.0",
        "next": "^12.3.1",
        "next-with-less": "^2.0.5",
        "prettier": "^2.7.1",
        "start-server-and-test": "^1.14.0",
        "ts-loader": "^9.4.1"
    },
    "scripts": {
        "dev": "next dev -p 8080 | pino-pretty",
        "dev-ingen-dekorator": "NO_DECORATOR=true npm run dev | pino-pretty",
        "interne": "SPINNSYN_FRONTEND_INTERNE=true NO_DECORATOR=true npm run dev | pino-pretty",
        "start": "next start -p 8080",
        "start-ingen-dekorator": "NO_DECORATOR=true npm start",
        "arkiv": "npm run build && npm run start-ingen-dekorator",
        "build": "next build",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "cy:open": "cypress open --e2e",
        "e2e": "start-test dev-ingen-dekorator http://localhost:8080/syk/sykepenger cy:open",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src cypress",
        "lint:fix": "eslint --ext=ts,tsx src cypress --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals",
            "plugin:cypress/recommended"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-non-null-assertion": "off",
            "@next/next/no-img-element": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
