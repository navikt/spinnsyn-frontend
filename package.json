{
    "name": "spinnsyn-frontend",
    "version": "0.1.1",
    "private": true,
    "engines": {
        "node": "20"
    },
    "dependencies": {
        "@grafana/faro-web-sdk": "^1.6.0",
        "@navikt/aksel-icons": "^6.7.0",
        "@navikt/ds-css": "^6.7.0",
        "@navikt/ds-react": "^6.7.0",
        "@navikt/ds-tailwind": "^6.7.0",
        "@navikt/ds-tokens": "^6.1.0",
        "@navikt/nav-dekoratoren-moduler": "^2.1.6",
        "@navikt/next-api-proxy": "^3.4.1",
        "@navikt/next-logger": "^1.24.0",
        "@navikt/oasis": "^3.2.4",
        "@sentry/nextjs": "^7.112.2",
        "@tanstack/react-query": "^5.29.2",
        "@testing-library/cypress": "^10.0.1",
        "@unleash/nextjs": "^1.4.2",
        "cookie": "^0.6.0",
        "dayjs": "^1.11.11",
        "html-react-parser": "^5.1.10",
        "jose": "^5.2.4",
        "next-logger": "^4.0.0",
        "openid-client": "^5.6.5",
        "pino-pretty": "^10.3.1",
        "prom-client": "^15.1.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "remeda": "^1.61.0",
        "tailwind-merge": "^2.3.0",
        "typescript": "^5.4.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^6.0.1",
        "@navikt/eslint-config-teamsykmelding": "^5.2.0",
        "@types/classnames": "^2.3.0",
        "@types/cookie": "^0.6.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.23",
        "@types/react": "^18.2.61",
        "@types/uuid": "^9.0.8",
        "autoprefixer": "^10.4.19",
        "axe-core": "^4.9.0",
        "cypress": "^13.8.1",
        "cypress-axe": "^1.5.0",
        "cypress-real-events": "^1.12.0",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.3",
        "eslint-plugin-cypress": "^2.15.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "next": "^14.2.3",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "start-server-and-test": "^2.0.3",
        "tailwindcss": "^3.4.3",
        "ts-loader": "^9.5.1"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 8080 | pino-pretty",
        "dev-ingen-dekorator": "MOCK_BACKEND=true NO_DECORATOR=true npm run dev | pino-pretty",
        "interne": "SPINNSYN_FRONTEND_INTERNE=true NO_DECORATOR=true npm run dev | pino-pretty",
        "start": "next start -p 8080",
        "start-ingen-dekorator": "NO_DECORATOR=true npm start",
        "arkiv": "npm run build && npm run start-ingen-dekorator",
        "build": "next build",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "cy:open": "cypress open --e2e",
        "e2e": "NO_DECORATOR=true start-test dev http://localhost:8080/syk/sykepenger cy:open",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src cypress",
        "lint:fix": "eslint --ext=ts,tsx src cypress --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals",
            "plugin:cypress/recommended"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-non-null-assertion": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
