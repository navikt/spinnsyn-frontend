{
    "name": "spinnsyn-frontend",
    "version": "0.1.1",
    "private": true,
    "dependencies": {
        "@navikt/aksel-icons": "^4.1.2",
        "@navikt/ds-css": "^4.1.2",
        "@navikt/ds-react": "^4.1.2",
        "@navikt/ds-react-internal": "^3.4.3",
        "@navikt/ds-tailwind": "^4.1.2",
        "@navikt/ds-tokens": "^4.1.2",
        "@navikt/nav-dekoratoren-moduler": "^2.0.1",
        "@navikt/next-api-proxy": "^3.2.0",
        "@navikt/next-logger": "^1.19.0",
        "@sentry/nextjs": "^7.54.0",
        "@tanstack/react-query": "^4.29.12",
        "cookie": "^0.5.0",
        "dayjs": "^1.11.8",
        "html-react-parser": "^4.0.0",
        "jose": "^4.13.1",
        "next-logger": "^3.0.2",
        "openid-client": "^5.4.2",
        "pino-pretty": "^10.0.0",
        "prom-client": "^14.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^1.12.0",
        "typescript": "^5.1.3",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^5.17.1",
        "@navikt/eslint-config-teamsykmelding": "^1.7.0",
        "@types/classnames": "^2.3.0",
        "@types/cookie": "^0.5.1",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.28",
        "@types/uuid": "^9.0.1",
        "autoprefixer": "^10.4.14",
        "axe-core": "^4.7.2",
        "cypress": "^12.13.0",
        "cypress-axe": "^1.4.0",
        "cypress-real-events": "^1.8.1",
        "eslint": "^8.41.0",
        "eslint-config-next": "^13.4.4",
        "eslint-plugin-cypress": "^2.13.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "next": "^13.4.4",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "start-server-and-test": "^2.0.0",
        "tailwindcss": "^3.3.2",
        "ts-loader": "^9.4.3"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 8080 | pino-pretty",
        "dev-ingen-dekorator": "MOCK_BACKEND=true NO_DECORATOR=true npm run dev | pino-pretty",
        "interne": "SPINNSYN_FRONTEND_INTERNE=true NO_DECORATOR=true npm run dev | pino-pretty",
        "start": "next start -p 8080",
        "start-ingen-dekorator": "NO_DECORATOR=true npm start",
        "arkiv": "npm run build && npm run start-ingen-dekorator",
        "build": "next build",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "cy:open": "cypress open --e2e",
        "e2e": "NO_DECORATOR=true start-test dev http://localhost:8080/syk/sykepenger cy:open",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src cypress",
        "lint:fix": "eslint --ext=ts,tsx src cypress --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals",
            "plugin:cypress/recommended"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-non-null-assertion": "off",
            "@next/next/no-img-element": "off",
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
