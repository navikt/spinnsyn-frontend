{
    "name": "spinnsyn-frontend",
    "version": "0.1.1",
    "private": true,
    "engines": {
        "node": "20"
    },
    "dependencies": {
        "@grafana/faro-web-sdk": "^1.3.5",
        "@navikt/aksel-icons": "^5.12.2",
        "@navikt/ds-css": "^5.12.2",
        "@navikt/ds-react": "^5.12.2",
        "@navikt/ds-tailwind": "^5.12.2",
        "@navikt/ds-tokens": "^5.7.6",
        "@navikt/nav-dekoratoren-moduler": "^2.1.5",
        "@navikt/next-api-proxy": "^3.4.0",
        "@navikt/next-logger": "^1.21.0",
        "@sentry/nextjs": "^7.91.0",
        "@tanstack/react-query": "^5.17.1",
        "@testing-library/cypress": "^10.0.1",
        "@unleash/nextjs": "^1.3.0",
        "cookie": "^0.6.0",
        "dayjs": "^1.11.10",
        "html-react-parser": "^5.0.11",
        "jose": "^5.2.0",
        "next-logger": "^3.0.2",
        "openid-client": "^5.6.2",
        "pino-pretty": "^10.3.1",
        "prom-client": "^15.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "remeda": "^1.33.0",
        "tailwind-merge": "^2.2.0",
        "typescript": "^5.3.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^6.0.1",
        "@navikt/eslint-config-teamsykmelding": "^5.1.0",
        "@types/classnames": "^2.3.0",
        "@types/cookie": "^0.5.1",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.5.4",
        "@types/react": "^18.2.22",
        "@types/uuid": "^9.0.5",
        "autoprefixer": "^10.4.16",
        "axe-core": "^4.8.3",
        "cypress": "^13.6.2",
        "cypress-axe": "^1.5.0",
        "cypress-real-events": "^1.11.0",
        "eslint": "^8.56.0",
        "eslint-config-next": "^14.0.4",
        "eslint-plugin-cypress": "^2.15.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "next": "^14.0.4",
        "postcss": "^8.4.32",
        "prettier": "^3.1.1",
        "prettier-plugin-tailwindcss": "^0.5.9",
        "start-server-and-test": "^2.0.3",
        "tailwindcss": "^3.4.0",
        "ts-loader": "^9.5.1"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 8080 | pino-pretty",
        "dev-ingen-dekorator": "MOCK_BACKEND=true NO_DECORATOR=true npm run dev | pino-pretty",
        "interne": "SPINNSYN_FRONTEND_INTERNE=true NO_DECORATOR=true npm run dev | pino-pretty",
        "start": "next start -p 8080",
        "start-ingen-dekorator": "NO_DECORATOR=true npm start",
        "arkiv": "npm run build && npm run start-ingen-dekorator",
        "build": "next build",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "cy:open": "cypress open --e2e",
        "e2e": "NO_DECORATOR=true start-test dev http://localhost:8080/syk/sykepenger cy:open",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src cypress",
        "lint:fix": "eslint --ext=ts,tsx src cypress --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals",
            "plugin:cypress/recommended"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-non-null-assertion": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
